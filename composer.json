{
    "name": "datashaman/phpunit",
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2"
    },
    "authors": [
        {
            "name": "Marlin Forbes",
            "email": "marlinf@datashaman.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Datashaman\\PHPUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datashaman\\PHPUnit\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "phpunit/php-timer": "^2.1"
    }
}
